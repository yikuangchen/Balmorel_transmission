;                                                                                                                                                                                                                                                                                                                                            
* Convert from €2015 to €2012                                                                                                                                                                                                                                                                                                                
TAX_F_EL_IN(YYY,CCC,FFF) = TAX_F_EL_IN(YYY,CCC,FFF) * 0.9799;                                                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                                                                                                             
* Apply 2015 numbers to all years:                                                                                                                                                                                                                                                                                                           
TAX_F_EL_IN(Y,C,FFF)$(YVALUE(Y) NE 2015) = TAX_F_EL_IN('2015',C,FFF);                                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                                                                                                             
* Assign values onto TAX_F_EL                                                                                                                                                                                                                                                                                                                
TAX_F_EL(Y,C,G)$((SUM(IA$ICA(C,IA), GKFX(Y,IA,G)) > 0) OR SUM(IA$ICA(C,IA), AGKN(IA,G)))  = SUM(FFF$IGF(G,FFF), TAX_F_EL_IN(Y,C,FFF));                                                                                                                                                                                                       
                                                                                                                                                                                                                                                                                                                                             
*** Secondary input: exceptions from the main input on technology level                                                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                                                                                                             
* Tax for electricity storage is zero                                                                                                                                                                                                                                                                                                        
*TAX_F_EL(Y,C,G)$(IGESTOS(G) OR IGESTOS(G)) =                                                                                                                                                                                                                        0       ;                                                               
                                                                                                                                                                                                                                                                                                                                             
* Tax for power to heat is zero                                                                                                                                                                                                                                                                                                              
TAX_F_EL(Y,C,G)$(GDATA(G,'GDTYPE') EQ GETOH) =                                                                                                                                                                                                                       0       ;                                                               
                                                                                                                                                                                                                                                                                                                                             
* CHP in Latvia are exempt from taxes                                                                                                                                                                                                                                                                                                        
TAX_F_EL(Y,'LATVIA',G)$((GDATA(G,'GDTYPE') EQ GBPR) OR (GDATA(G,'GDTYPE') EQ GEXT)) =                                                                                                                                                                                                                0        ;                              
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                             
